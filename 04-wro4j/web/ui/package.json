{
  "name": "web-ui",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "precompile": "rimraf -rf ../src/main/resources/static",
    "compile": "webpack --config=./config/webpack.config.babel.js",
    "build": "npm run compile -- --env production",
    "browser-sync": "browser-sync -c ./config/bs-config.js start",
    "serve-browser-sync": "npm-run-all build browser-sync",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack.config.babel.js --env development",
    "sleep": "sleep 3",
    "open": "open-url localhost:8000/",
    "open-url": "npm-run-all sleep open",
    "start": "concurrently --kill-others 'npm-run-all -p webpack-dev-server open-url'",
    "lint": "eslint -c ./config/.eslintrc.js --ignore-pattern '**/*.spec.js' 'config/**/*.js' 'src/**/*.js'",
    "pretest": "npm run compile -- --env development",
    "test": "karma start ./config/karma.conf.js #--log-level=debug",
    "preserve": "yarn build",
    "serve:livereload": "livereload | true",
    "serve:serve": "serve -C -p 3000 -s ../src/main/resources/static",
    "serve": "yarn -i; npm-run-all -p serve:*"
  },
  "dependencies": {
    "angular": "1.6.6",
    "angular-animate": "1.6.6",
    "angular-aria": "1.6.6",
    "angular-ui-router": "1.0.3",
    "bootstrap": "3.3.7",
    "jquery": "3.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.43",
    "angular-mocks": "1.6.6",
    "autoprefixer": "7.1.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "base-href-webpack-plugin": "1.0.2",
    "browser-sync": "2.18.8",
    "compression-webpack-plugin": "0.3.2",
    "concurrently": "3.4.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.4",
    "cssnano": "3.10.0",
    "editorconfig": "0.13.2",
    "eslint": "3.17.1",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "2.1.0",
    "faker": "4.1.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "jasmine": "2.5.3",
    "json-loader": "0.5.7",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "livereload": "0.6.2",
    "ng-annotate-loader": "0.6.1",
    "ng-cache-loader": "0.0.25",
    "npm-run-all": "4.1.1",
    "open-url": "2.0.2",
    "postcss": "5.2.16",
    "postcss-loader": "1.3.3",
    "precss": "2.0.0",
    "progress-bar-webpack-plugin": "1.10.0",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.1.0",
    "rimraf": "2.6.2",
    "rucksack-css": "1.0.2",
    "script-ext-html-webpack-plugin": "1.8.5",
    "serve": "6.1.0",
    "sinon": "1.17.7",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "stylelint": "7.9.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.8.2"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "syntax-dynamic-import"
    ]
  },
  "livereload": {
    "exclusions": [
      "*bower_components*",
      "*node_modules*",
      "*package-lock.json",
      "*yarn.lock"
    ]
  },
  "keywords": [
    "angularjs",
    "webpack 2"
  ],
  "main": "index.js",
  "author": "daggerok",
  "description": "angularjs webpack 2 quickstart",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs#readme"
}
